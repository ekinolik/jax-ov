# Massive.com API Key
# Get your API key from https://massive.com
MASSIVE_API_KEY=your_api_key_here

# Log backup configuration
# Number of trading days to keep on disk (required for backup script)
LOG_RETENTION_DAYS=30

# S3 path for log backups in format: bucket/prefix (required for backup script)
# Example: my-bucket/logs-backup
AWS_S3_PATH=your-bucket/your-prefix

# Apple Sign-In configuration (required for authentication)
APPLE_CLIENT_ID=your_apple_client_id
APPLE_TEAM_ID=your_apple_team_id
APPLE_PRIVATE_KEY=your_apple_private_key_pem

# JWT configuration (required for authentication)
JWT_SECRET=your_jwt_secret_key
JWT_EXPIRY_HOURS=168
